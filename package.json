{
  "//1": "describes your app and its dependencies",
  "//2": "https://docs.npmjs.com/files/package.json",
  "//3": "updating this file will download and update your packages",
  "name": "react-babel",
  "version": "0.0.1",
  "description": "A boilerplate for getting a web application off the ground using React and Babel for JSX.",
  "main": "server.js",
  "scripts": {
    "start": "rimraf ./public && webpack --progress && babel-register server"
  },
  "dependencies": {
    "express": "^4.15.3",
    "react": "^15.5.3",
    "react-dom": "^15.5.3",
    "emotion": "^9.1.1",
    "@hackoregon/component-library": "2.0.0",
    "react-leaflet": "^1.1.6",
    "react-dimensions": "^1.3.1",
    "styled-components": "^1.4.2",
    "react-select": "^1.0.0-rc.3",
    "webpack-blocks": "^1.0.0",
    "autoprefixer": "^6.7.7",
    "leaflet": "^1.0.3",
    "prop-types": "^15.6.1",
    "react-transition-group": "^1.1.2",
    "react-addons-css-transition-group": "^15.4.1",
    "react-addons-shallow-compare": "^15.4.1",
    "chai": "^3.5.0",
    "enzyme": "^2.7.0",
    "react-addons-test-utils": "^15.4.1",
  },
  "devDependencies": {
    "babel-core": "^6.0.0",        
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "babel-register-cli": "^4.0.0",
    "css-loader": "^0.27.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "*",
    "rimraf": "^2.6.1",
    "webpack": "^2.5.0",
    "webpack-dev-server": "^2.5.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.22.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "style-loader": "^0.16.1",
    "webpack-blocks": "^1.0.0",
    "autoprefixer": "^6.7.7",
    "chalk": "^1.1.3"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-2"
    ],
    "plugins": [
      "react-hot-loader/babel",
      "transform-class-properties",
      [ "emotion", { "sourceMap": true, "inline": true, "autoLabel": true }]
    ]
  },
  "engines": {
    "node": "8.11.1"
  },
  "repository": {
    "url": "https://glitch.com/edit/#!/react-babel"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "express",
    "react",
    "civic",
    "hack oregon",
    "open data",
    "data",
    "systems",
    "data visualization",
    "art"
  ]
}